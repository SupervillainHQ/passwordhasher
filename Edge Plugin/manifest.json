{
    "name": "Copy Domain",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to copy the domain to ClipBoard.",
    "icons":{
    	"16": "icons/domain.16x16.png",
    	"32": "icons/domain.32x32.png",
    	"48": "icons/domain.48x48.png",
    	"128": "icons/domain.128x128.png"
    },
    "action": {
    	"default_popup" : "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}